{
  "name": "@scripta/server",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "tsup": {
    "entry": [
      "src/server.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "scripts": {
    "dev": "tsx watch --env-file .env src/server.ts",
    "build": "tsup",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts --fix",
    "test:watch": "jest --setupFiles dotenv/config --watch",
    "studio": "prisma studio",
    "reset": "prisma migrate reset",
    "migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "@prisma/client": "^5.9.0",
    "@types/node-cron": "^3.0.11",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "fastify": "^5.2.1",
    "fastify-type-provider-zod": "^4.0.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "prisma": "^5.10.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.8",
    "tsup": "^8.2.4",
    "tsx": "^4.19.3"
  }
}
